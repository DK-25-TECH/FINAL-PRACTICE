#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;
    struct node *prv;
}node;
node *create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    nn->prv=NULL;
    return nn;
}

void inslst(node **head,int data)
{
    node *nn=create(data);
    node *tp=*head;
    if(*head==NULL)
    {
        *head=nn;
        return;
    }
    while(tp->next!=NULL)
    {
        tp=tp->next;
    }
    tp->next=nn;
    nn->prv=tp;
}

void display(node *head)
{
    node *tp=head;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}

void swap(node **head)
{
    node *tp=*head;
    while(tp!=NULL && tp->next!=NULL)
    {
        int x=tp->data;
        tp->data=tp->next->data;
        tp->next->data=x;
        tp=tp->next->next;
    }
}
int main()
{
    node *head=NULL;
    inslst(&head,1);
    inslst(&head,2);
    inslst(&head,3);
    inslst(&head,4);
    inslst(&head,5);
    inslst(&head,6);
    display(head);
    swap(&head);
    printf("\n");
    display(head);
}
