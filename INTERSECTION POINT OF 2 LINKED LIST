#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;
}node;

node* create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}

void insLast(node **head,int data)
{
    node *nn=create(data);
    if(*head==NULL)
    {
        nn->next=*head;
        *head=nn;
        return;
    }
    node *tp=*head;
    while(tp->next!=NULL)
    {
        tp=tp->next;
    }
    tp->next=nn;
}

void display(node *head)
{
    node *tp=head;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}



node *intersectList(node *head1,node *head2)
{
    node *x=head1;
    node *y=head2;
    while(x!=y)
    {
        if(x==NULL)
        {
            x=head2;
        }
        else
        {
            x=x->next;
        }
        if(y==NULL)
        {
            y=head1;
        }
        else
        {
            y=y->next;
        }
    }
    return x;
}
int main()
{
    node *head1=NULL;
    node *head2=NULL;
    
    insLast(&head1,1);
    insLast(&head1,2);
    insLast(&head1,3);
    insLast(&head1,4);
    insLast(&head1,5);
    
    insLast(&head2,4);
    insLast(&head2,7);
    insLast(&head2,8);
    insLast(&head2,9);
    insLast(&head2,1);
    
    head2->next->next->next->next=head1->next->next;
    
    node *x=intersectList(head1,head2);
    
    display(x);
    
}
