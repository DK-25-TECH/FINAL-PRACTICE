#include<stdio.h>
#include<stdlib.h>
typedef struct node{
     int data;
     struct node *next;
    
}node;

node *create(int data)
{
    node *nn=malloc(sizeof(struct node));
    nn->data=data;
    nn->next=NULL;
    return nn;
}
void enqueue(node **front,node **rear,int data)
{
    node *nn=create(data);
    if(*rear==NULL && *front==NULL)
    {
        *front=nn;
        *rear=nn;
        return;
    }
    (*rear)->next=nn;
    *rear=nn;
}

void dequeue(node **front,node **rear)
{
    if(*front==NULL)
    {
        return;
    }
    node *tp=*front;
    (*front)=tp->next;
    if(*front==NULL)
    {
        *rear=NULL;
    }
    free(tp);
}
void display(node *front)
{
    node *tp=front;
    while(tp!=NULL)
    {
        printf("%d ",tp->data);
        tp=tp->next;
    }
}

void peek(node *front)
{
    node *tp=front;
    if(tp==NULL)
    {
        return;
    }
    printf("%d ",tp->data);
}
int main()
{
    node *front=NULL,*rear=NULL;
    enqueue(&front,&rear,10);
    enqueue(&front,&rear,20);
    enqueue(&front,&rear,30);
    enqueue(&front,&rear,40);
    enqueue(&front,&rear,50);
    
    display(front);
    printf("\n");
    
    dequeue(&front,&rear);
    display(front);
    printf("\n");
    peek(front);
}
